# Database Connection
DATABASE_URL=postgresql://postgres:password@localhost:5432/carsdb
PGUSER=postgres
PGHOST=localhost
PGPASSWORD=password
PGPORT=5432
PGDATABASE=carsdb

# Session Secret (Required for authentication)
SESSION_SECRET=your_secure_random_string_here

# Car API Credentials (Optional - application uses fallback data if not provided)
CAR_API_KEY=your_api_key
CAR_API_SECRET=your_api_secret
CAR_API_TOKEN=your_api_token

# Node Environment
NODE_ENV=production

# Server Configuration
PORT=5000